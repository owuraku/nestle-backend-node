{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"camelcase": "error",
		"max-lines": ["error", 150]
	},
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	}
}
